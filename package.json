{
  "name": "git-geass",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.3",
  "description": "üõ†Ô∏è Some quick actions for git.",
  "author": "YunYouJun <me@yunyoujun.cn>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/YunYouJun",
  "homepage": "https://github.com/YunYouJun/git-geass#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunYouJun/git-geass.git"
  },
  "bugs": "https://github.com/YunYouJun/git-geass/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "gitg": "bin/index.mjs",
    "git-geass": "bin/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "tsx src/run.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "c12": "^2.0.1",
    "consola": "^3.2.3",
    "date-fns": "^4.1.0",
    "prompts": "^2.4.2",
    "simple-git": "^3.27.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.9.0",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.12.3",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
